<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="nossolarPU" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>br.com.bonfimsoft.nossolar.model.Atendimento</class>
		<class>br.com.bonfimsoft.nossolar.model.Compromisso</class>
		<class>br.com.bonfimsoft.nossolar.model.Entrevista</class>
		<class>br.com.bonfimsoft.nossolar.model.Feriado</class>
		<class>br.com.bonfimsoft.nossolar.model.Livro</class>
		<class>br.com.bonfimsoft.nossolar.model.Paciente</class>
		<class>br.com.bonfimsoft.nossolar.model.Perfil</class>
		<class>br.com.bonfimsoft.nossolar.model.Problema</class>
		<class>br.com.bonfimsoft.nossolar.model.TipoProblema</class>
		<class>br.com.bonfimsoft.nossolar.model.TipoTratamento</class>
		<class>br.com.bonfimsoft.nossolar.model.Trabalhador</class>
		<class>br.com.bonfimsoft.nossolar.model.Tratamento</class>
		<exclude-unlisted-classes />
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect" />
		</properties>
	</persistence-unit>
</persistence>
